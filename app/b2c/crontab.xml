<cronentries>
  <cron id="b2c_tasks_cleancartobject">
    <description>清除用户购物车数据</description>
    <schedule>0 2 1 * *</schedule>
    <enabled>true</enabled>
  </cron>
  <cron id="b2c_tasks_order_cancel">
    <description>自动取消营销过期订单</description>
    <schedule>5 * * * *</schedule>
    <enabled>true</enabled>
  </cron>
  <cron id="b2c_tasks_order_cancel_unpay">
    <description>取消过期普通订单</description>
    <schedule>50 * * * *</schedule>
    <enabled>true</enabled>
  </cron>
  <cron id="b2c_tasks_order_delivery">
    <description>自动确认收货</description>
    <schedule>5 * * * *</schedule>
    <enabled>true</enabled>
  </cron>
  <cron id="b2c_tasks_order_check_payment">
    <description>检查已支付订单是否已经变更订单支付状态</description>
    <schedule>55 * * * *</schedule>
    <enabled>true</enabled>
  </cron>
